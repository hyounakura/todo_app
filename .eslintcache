  })\n    todoList.splice(index, 1);\n    this.setState({todoList: todoList})\n  }\n\n  render() {\n    const todoListNode = this.state.todoList.map(element => {\n      return (\n        <TodoElement element={element} key={element.id} onDelete={() => this.handleDelete()}/>\n      )\n    })\n    return <div>\n      <h1>TO DO APP</h1>\n      <AddTodo \n        {...this.state}\n        onChange={e => this.onChange(e)}\n        add={(element) => this.add(element)}\n      />\n      <ul>\n       {todoListNode}\n      </ul>\n    </div>;\n  }\n}\n\nexport default TodoApp;","/Users/nakurahyou/react/todo_app/src/AddTodo.js",[],"/Users/nakurahyou/react/todo_app/src/TodoElement.js",[],"/Users/nakurahyou/react/todo_app/src/reportWebVitals.js",[],{"ruleId":"24","severity":1,"message":"25","line":29,"column":33,"nodeType":"26","messageId":"27","endLine":29,"endColumn":35},"array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside"]